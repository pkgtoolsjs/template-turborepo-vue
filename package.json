{
  "name": "template-turborepo-vue",
  "version": "1.2.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "node -e \"try{require('./scripts/postinstall.js')}catch(e){}\"",
    "init": "node ./scripts/init.js",
    "test": "turbo run test",
    "build": "turbo run build",
    "docker:build": "mkdir -p .logs/docker-compose && docker compose build 2>&1 | tee .logs/docker-compose/build.log",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "up:all": "pnpm update --recursive"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@types/yargs": "^17.0.33",
    "chalk": "^5.4.1",
    "consola": "^3.4.2",
    "cpy": "^11.1.0",
    "execa": "^9.6.0",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.0.0",
    "npm-run-all2": "^8.0.4",
    "plop": "^4.0.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.13.0",
    "turbo": "^2.5.3",
    "yaml": "^2.8.0",
    "yargs": "^17.7.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
