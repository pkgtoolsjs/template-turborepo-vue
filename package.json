{
  "name": "template-turborepo-vue",
  "version": "1.2.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "node -e \"try{require('./scripts/postinstall.js')}catch(e){}\"",
    "init": "node ./scripts/init.js",
    "test": "turbo run test",
    "build": "turbo run build",
    "docker:build": "mkdir -p .logs/docker-compose && docker compose build 2>&1 | tee .logs/docker-compose/build.log",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "up:all": "pnpm update --recursive"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@prettier/plugin-oxc": "^0.0.4",
    "@types/yargs": "^17.0.33",
    "chalk": "^5.6.2",
    "consola": "^3.4.2",
    "cpy": "^12.0.1",
    "execa": "^9.6.0",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.2.0",
    "npm-run-all2": "^8.0.4",
    "plop": "^4.0.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.13.1",
    "turbo": "^2.5.7",
    "yaml": "^2.8.1",
    "yargs": "^18.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
