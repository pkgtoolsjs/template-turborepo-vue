{
  "name": "@template-turborepo-vue/server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix .",
    "format": "prettier --write .",
    "test": "echo \"OK\" && exit 0",
    "dev": "tsx watch --env-file=.env ./src/index.ts --nolazy",
    "start": "node --env-file=.env ./dist/index.js",
    "type-check": "tsc --noEmit",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "tsdown"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/middie": "^9.0.3",
    "@template-turborepo-vue/math": "workspace:*",
    "fastify": "^5.4.0"
  },
  "devDependencies": {
    "@template-turborepo-vue/eslint-config": "workspace:*",
    "@template-turborepo-vue/tsconfig": "workspace:*",
    "@types/node": "~24.2.1",
    "eslint": "^9.33.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "tsdown": "^0.13.4",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "~5.9.2"
  }
}
